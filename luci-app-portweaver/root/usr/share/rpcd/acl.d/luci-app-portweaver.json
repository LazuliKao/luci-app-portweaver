{
	"luci-app-portweaver": {
		"description": "Grant UCI and ubus access for LuCI PortWeaver app",
		"read": {
			"uci": ["portweaver"],
			"ubus": {
				"portweaver": ["get_status", "list_projects"]
			}
		},
		"write": {
			"uci": ["portweaver"],
			"ubus": {
				"portweaver": ["set_enabled"]
			}
		}
	}
}
